<?xml version="1.0" encoding="UTF-8"?>
<interface title="home-title" header="home-header">
	<container>
		<section title="home-generate-title">
			<container>
				<textEdit title="home-contextsWithAssessments" rows="1" cols="12">
					<model ref="gspecs.contextsWithAssessments" />
				</textEdit>
				<textEdit title="home-assessmentsPerContext" rows="1" cols="12">
					<model ref="gspecs.assessmentsPerContext" />
				</textEdit>
				<textEdit title="home-submissionsPerStudent" rows="1" cols="12">
					<model ref="gspecs.submissionsPerStudent" />
				</textEdit>
				<textEdit title="home-contextStudents" rows="1" cols="12">
					<model ref="gspecs.contextStudents" />
				</textEdit>
				<textEdit title="home-itemsPerAssessment" rows="1" cols="12">
					<model ref="gspecs.itemsPerAssessment" />
				</textEdit>
				<navigationBar>
					<container>
						<navigation style="BUTTON" destination="/generate" title="home-generate" submit="TRUE" />
					</container>
				</navigationBar>
			</container>
		</section>
		<section title="home-simulate-title">
			<container>
				<textEdit title="home-numUsers" rows="1" cols="12">
					<model ref="sspecs.numUsers" />
				</textEdit>
				<textEdit title="home-startGap" rows="1" cols="12">
					<model ref="sspecs.startGap" />
				</textEdit>
				<textEdit title="home-thinkTime" rows="1" cols="12">
					<model ref="sspecs.thinkTime" />
				</textEdit>
				<navigationBar>
					<container>
						<navigation style="BUTTON" destination="/simulate" title="home-simulate" submit="TRUE" />
					</container>
				</navigationBar>
			</container>
		</section>
		<section title="home-install-title">
			<container>
				<textEdit title="home-context" rows="1" cols="12">
					<model ref="ispecs.context" />
				</textEdit>
				<navigationBar>
					<container>
						<navigation style="BUTTON" destination="/install" title="home-install" submit="TRUE" />
						<navigation style="BUTTON" destination="/install_all" title="home-install-all" submit="TRUE" />
					</container>
				</navigationBar>
			</container>
		</section>
	</container>
</interface>
