<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

	<bean id="org.muse.mneme.tool.ErrorView"
			class="org.muse.mneme.tool.ErrorView"
			init-method="init"
			destroy-method="destroy"
			singleton="true">

 		<property name="ui"><ref bean="org.muse.ambrosia.api.UiService"/></property>
 
 		<property name="toolId"><value>sakai.mneme.test</value></property>
 		<property name="viewId"><value>error</value></property>
 		<property name="bundle"><value>mneme-test-tool</value></property>
	</bean>

</beans>
