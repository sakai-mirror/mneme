<interface title="pools-add-title" header="pools-add-header">
	<!--mode bar-->
	<modeBar>
		<container>
			<navigation destination="/tests"
				title="test-manager-nav-title" style="LINK"
				accessKey="test-manager-nav-access-key"
				description="test-manager-nav-description" />
			<navigation destination="/grades" title="grading-nav-title"
				style="LINK" accessKey="grading-nav-access-key"
				description="grading-nav-description" />
			<navigation destination="/pools" title="pools-nav-title"
				style="LINK" accessKey="pools-nav-access-key"
				description="pools-nav-description" disabled="TRUE" />
			<navigation destination="/list"
				title="student-view-nav-title" style="LINK"
				accessKey="student-view-nav-access-key"
				description="student-view-nav-description" />
		</container>
	</modeBar>
	<container>
	
	
		<!--navigation-->
		<navigationBar width="100%">
			<container>
				<navigation destination="/pools"
					title="save-title" style="BUTTON" accessKey="save-access"
					description="save-description" submit="FALSE">
					<icon icon="/icons/save.png" style="LEFT" />
				</navigation>
				<navigation destination="/pools" title="cancel" style="BUTTON"
					accessKey="cancel-access" description="cancel-description">
					<icon icon="/icons/cancel.gif" style="LEFT" />
				</navigation>
			</container>
		</navigationBar>	
	</container>
</interface>
